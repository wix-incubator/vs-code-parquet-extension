{
  "name": "vscode-parquet-viewer",
  "displayName": "Parquet File Viewer",
  "description": "VS Code extension to view .parquet files",
  "publisher": "gerda",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
		"onCustomEditor:parquetViewer.view"
	],
  "main": "./out/extension.js",
  "repository": {
    "url": "https://github.com/wix-incubator/vs-code-parquet-extension.git"
  },
  "contributes": {
    "customEditors": [
			{
				"viewType": "parquetViewer.view",
				"displayName": "Parquet File",
				"selector": [
					{
						"filenamePattern": "*.parquet"
					}
				]
			}
		]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "parquetjs-lite": "^0.8.7"
  }
}
